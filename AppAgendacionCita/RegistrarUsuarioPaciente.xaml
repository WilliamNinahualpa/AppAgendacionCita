<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AppAgendacionCita.RegistrarUsuarioPaciente">
    <ContentPage.Content>
        <StackLayout>
            <Entry x:Name="txtUsuario" Placeholder="Ingrese usuario"></Entry>
            <Entry x:Name="txtClave" Placeholder="Ingrese clave" IsPassword="True"></Entry>
            <Button x:Name="btnGuardarUsuarioP" Text="Guardar" Clicked="btnGuardarUsuarioP_Clicked"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>