<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AppAgendacionCita.VistaClinicaIngresar"
             >
    <ContentPage.Content>
        <StackLayout>
            <Entry Placeholder="Nombre de la clínica" x:Name="txtNombre"></Entry>
            <Entry Placeholder="Dirección de la clínica" x:Name="txtDireccion"></Entry>
            <Entry Placeholder="Correo de la clínica" x:Name="txtCorreo"></Entry>
            <Entry Placeholder="Telefono de la clínica" x:Name="txtTelefono"></Entry>
            <Entry Placeholder="Página web de la clínica" x:Name="txtPagina"></Entry>
            <Entry Placeholder="Representante de la clínica" x:Name="txtRepresentante"></Entry>
            <Picker x:Name="pickerPaises"></Picker>
            <Button Text="Registrar" x:Name="btnRegistrar" Clicked="btnRegistrar_Clicked"></Button>
            <Button Text="Regresar" x:Name="btnRegresar" Clicked="btnRegresar_Clicked"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>